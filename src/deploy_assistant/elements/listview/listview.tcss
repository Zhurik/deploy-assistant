.selected {
    background: blue;
    color: white;
}

.error {
    background: red;
    color: white;
}

.clean {
    background: grey;
    color: white;
}

Collapsible {
    width: 100%;
}

Collapsible Label {
    width: 100%;
}

/* Pipeline status colors */
Label.pipeline-success {
    color: green;
}

Label.pipeline-failed {
    color: red;
}

Label.pipeline-running {
    color: orange;
}

Label.pipeline-pending {
    color: yellow;
}
